#!/bin/bash
set -ueo pipefail

# Source config script
LEARNLAB_HOME="$( cd "$( dirname "${BASH_SOURCE[0]}" )/.." > /dev/null && pwd )"

mkdir -p "$LEARNLAB_HOME/data/AMR-Bank-v1.6"
pushd "$LEARNLAB_HOME/data/AMR-Bank-v1.6"
for split in training dev test; do
    file=amr-bank-struct-v1.6-$split.txt
    if [ ! -f $file ]; then
        wget http://amr.isi.edu/download/$file
    fi
done
popd
# "${LEARNLAB_HOME}/scripts/TRAIN.sh"


