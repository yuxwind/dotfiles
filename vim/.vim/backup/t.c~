#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define N  10000

int map[256];
char str[N]; 
int remove[256];
int oddsum = 0;
int evensum = 0;

int main(){

    while(scanf("%s",str)!=EOF)
    {
        memset(map,0,sizeof(int)*256);
        memset(remove,0,sizeof(int)*256);
        int i = 0;
        int len = strlen()
        for(i=0;i<len;i++)
        {
            map[str[i]]++;
        }
        int j = 0;
        for (j=0;j<256;j++)
        {
            if(map[j]==0)
                continue;
            else {
                if(map[j]%2)!=0)
                    remove[map[j]=map[j];

            }
        }
        int k =0;

        printf("%s\n",str);
    }
    return 0;
}
